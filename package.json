    "@types/react-redux": "^4.4.38",
    "@types/redux": "^3.6.0",
    "babel-jest": "19.0.0",
    "babel-preset-react-native": "1.9.1",
    "electron": "^1.6.2",
    "jest": "19.0.2",
    "npm-run-all": "^4.0.2",
